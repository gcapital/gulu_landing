/* CSS Document */
html {
	background: url(../images/bg_header_corkboard.png) repeat left top;
	margin: 0px;
	padding: 0px;
}

body {
	margin: 0px;
	padding: 0px;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	background: url(../images/bg_header_gradient.png) repeat-x center top;
}

h1, h2, h3, h4, h5, h6, h7, p, form, input, ul, li {
	padding: 0px;
	margin: 0px;
	font-weight: normal;
}

h1, h2, h3, h4, h5 {
	font-family: "Courier New", "Courier", serif;
}

h1 {
	font-size: 24px;
	font-weight: bold;
}

h2 {
	font-size: 22px;
	font-weight: bold;
}

h3, h4, h5 {
    font-size: 21px;
    font-weight: bold;
    margin-bottom: 10px;
}

p {
	line-height: 16px
}

ul, ol, li {
	list-style: none;
}

input, select, textarea {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	color: #000;
}

a {
	text-decoration: none;
	color: #000000;
	outline: none;
}

a:hover {
	/*text-decoration: underline;*/
	color: #000000;
	outline: none;
}

a img {
	border: 0px;
	outline: none;
}

:link, visited, ins {
    text-decoration: none;
}

.fleft {
	float: left;
}

.fright {
	float: right;
}

.overflow {
	overflow: auto;
}

.overflow_h {
	overflow: hidden;
}

.display_none {
    display: none;
}

.display_block {
    display: block;
}

.cf:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}
.cf {display: inline-block;}

/* Hides from IE-mac \*/
* html .cf {height: 1%;}
.cf {display: block;}
/* End hide from IE-mac */

/* ======================================== Master Styles ============================================ */
#wrapper {
	width: 1100px;
	margin: 0px auto;
	padding: 0;
	position: relative;
	background: url(../images/bg_content_paper.png) repeat-y center top;
}

/* ======================================== Form Elements ============================================ */

textarea, input[type="text"], input[type="password"] {
    background-color: #F9F9F9;
    border: 1px solid #B7B7B7;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    -moz-box-shadow: 0px 0px 3px #CCC;
    -webkit-box-shadow: 0px 0px 3px #CCC;
    box-shadow: 0px 0px 3px #CCC;
    padding: 5px;
    font-size: 12px;
    behavior: url(ie-css3.htc);
    color:#9e9e9e;
}

.pre_label, label {
    font-weight: bold;
    font-size: 14px;
    font-family: "Courier New", "Courier", serif;
}

.pre_label {
    color: #02b9e7;
    margin-left: 3px;
}

.search_form {
    color: #777777;
    font-size: 12px;
    line-height: 14px;
    margin: 0;
    padding: 0;
}

.search_form input {
    border: 0;
    float: left;
}

.search_form .keywords {
    width: 129px;
    background: url(../images/bg_search_keywords.png) no-repeat left top;
    padding: 9px 0px 10px 10px;
}

.search_form .location {
    width: 212px;
    background: url(../images/bg_search_location.png) no-repeat left top;
    padding: 9px 0 10px 10px;
    margin-left: 3px;
}

.search_form .submit {
    width: 103px;
    height: 32px;
    background: url(../images/btn_search.png) no-repeat left top;
    cursor: pointer;
    float: left;
    margin-left: 3px;
}

.input_medium {
    width: 300px;
}

/* date time picker */
.gulu_datetimepicker_date {
    background-color: #F9F9F9;
    border: 1px solid #B7B7B7;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
    border-radius: 2px;
    -moz-box-shadow: 0px 0px 3px #CCCCCCC;
    -webkit-box-shadow: 0px 0px 3px #CCCCCC;
    box-shadow: 0px 0px 3px #CCCCCC;
    padding: 5px;
    font-size: 14px;
    margin: 10px 0px;
    height: 25px;
    margin-right: 10px;
}

.gulu_datetimepicker_date input {
    background: none;
    border: 0px;
    margin: 0;
    padding: 0;
    -moz-border-radius: 0px;
    -webkit-border-radius: 0px;
    border-radius: 0px;
    -moz-box-shadow: 0px 0px 0px #CCCCCCC;
    -webkit-box-shadow: 0px 0px 0px #CCCCCC;
    box-shadow: 0px 0px 0px #CCCCCC;
    height: 25px;
}

.gulu_datetimepicker_date button {
    border: 0;
    margin: -3px 0px 0px 0px;
    padding: 0;
    background: none;
    cursor: pointer;
    float: right;
    width: 34px;
    height: 34px;
}

.gulu_datetimepicker_time_input {
    width: 150px;
    height: 25px;
	margin: 10px 0;
	padding: 6px;
}

.gulu_datetimepicker_date{
	float: left;
}

