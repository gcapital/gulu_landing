/* ======================================== User Ranking Styles ============================================ */
.user_ranking .header * {
    font-family: "Courier New", "Courier", serif;
}

.user_ranking .header {
	position: relative;
	height: 75px;	
}

.user_ranking .header h2 {
	position: absolute;
	background: url(../images/bg_ranking_tab.png) no-repeat;
	width: 217px;
	height: 47px;
	padding-top: 28px;
	text-align: center;
	z-index: 5;
}

.user_ranking .header h2:hover {
	cursor: pointer;
}

.user_ranking .header h2.restaurant_tab {
    left: 20px;
}

.user_ranking .header h2.dish_tab {
	left: 215px;
}

.user_ranking .header h2.top {
	z-index: 12;
}

.user_ranking .header span {
	background: url(../images/bg_ranking_split_bar.png) no-repeat;
	width: 742px;
	height: 23px;
	display: block;
	position: absolute;
	top: 50px;
	left: -6px;
	z-index: 9;
}

.user_ranking .list {
    margin: 10px auto 0;
    width: 660px;
    padding-bottom:50px;
}

.user_ranking .list li {
	width: 660px;
	height: 50px;
	border: 1px #A1A1A1 solid;
	-moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
    margin-bottom: 8px;
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
    box-shadow: none;
    behavior: url(ie-css3.htc);
    background-color: #fff;
    position: relative;
    cursor: move;
}

.user_ranking .list li .rank {
	-moz-border-radius: 10px 0 0 10px;
    -webkit-border-radius: 10px 0 0 10px;
    border-radius: 10px 0 0 10px;
    width: 60px;
    height: 35px;
    background-color: #e6e6e6;
    display: block;
    behavior: url(ie-css3.htc);
    text-align: center;
    padding: 15px 0 0 3px;
    font-size: 20px;
    float: left;
    font-weight: bold;
    font-family: "Courier New", "Courier", serif;
}

.user_ranking .list li .info {
	height: 34px;
	padding: 10px 0 0 20px;
	float: left;
	width: 250px;
}

.user_ranking .list li .info h4,
.user_ranking .list li .info h5 {
	font-size: 13px;
	margin-bottom: 2px;
}

.user_ranking .list li .info h5 {
	color: #999;
	font-weight: normal;
}

.user_ranking .list li .city {
	margin: 20px 0 0 30px;
	float: left;
	font-family: "Courier New", "Courier", serif;
	color: #999;
}

.user_ranking .list li .buttons {
	float: left;
	margin-left: 40px;
}

.user_ranking .list li .buttons .btn2 {
	float: left;
	margin: 13px 2px 0 0;
}

.user_ranking .list li .cross {
    display: none;
}

.user_ranking .list li .saving {
	display:none;
    position: absolute;
    top: 20px;
    left: 250px;
	color: #4ECEFB;
}

.user_ranking .list li.hover {
    width: 658px;
    height: 48px;
    border: 2px #4ECEFB solid;
    -moz-box-shadow: 0 2px 3px 1px #aaa;
    -webkit-box-shadow: 0 2px 3px 1px #aaa;
    box-shadow: 0 2px 3px 1px #aaa;
}

.user_ranking .list li.hover .rank {
    -moz-border-radius: 8px 0 0 8px;
    -webkit-border-radius: 8px 0 0 8px;
    border-radius: 8px 0 0 8px;
    width: 60px;
    height: 34px;
    behavior: url(ie-css3.htc);
    padding: 14px 0 0 2px;
    background-color: #fff;
    color: #4ECEFB;
}

.user_ranking .list li.hover .info {
    padding: 9px 0 0 20px;
}

.user_ranking .list li.hover .city {
    margin: 19px 0 0 30px;
}

.user_ranking .list li.hover .buttons .btn2 {
    margin: 12px 2px 0 0;
}

.user_ranking .list li.hover .cross {
	display: block;
	background: url(../images/icon_cross_blue.png) no-repeat;
	height: 21px;
	width: 21px;
	margin: 15px 0 0 30px;
	float: left;
	cursor: move;
}
