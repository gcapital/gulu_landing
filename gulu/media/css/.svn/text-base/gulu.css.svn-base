.btn1, .btn2, .btn3, .btn4, .btn5, .btn6 {
	border: medium none;
    font-size: 13px;
    font-weight: bold;
    font-family: "Courier New", "Courier", serif;
    display:block;
    text-align: center;
    /*word-spacing: -4px;*/
}

.btn1 {
	background: url(../images/btn_bg_light29.png) no-repeat;
    width: 29px;
    height: 28px;
}

.btn2 {
	background: url(../images/btn_bg_light54.png) no-repeat;
	width: 54px;
}

.btn3 {
    background: url(../images/btn_bg_light73.png) no-repeat;
