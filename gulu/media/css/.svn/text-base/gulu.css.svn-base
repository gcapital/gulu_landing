/* like styles */

.like_btn {
	width: 59px;
	height: 19px;
    background: url(../images/btn_unlike.png) no-repeat;
    display: block;
    cursor: pointer;
}

.unlike_btn {
	width: 59px;
    height: 19px;
    background: url(../images/btn_like.png) no-repeat;
    display: block;
    cursor: pointer;
}

.like_info {
	background-color: #FEFDBA;
    -moz-border-radius: 5px; /* Firefox */
    -webkit-border-radius: 5px; /* Safari and Chrome */
    border-radius: 5px; /* Opera 10.5+, future browsers, and now also Internet Explorer 6+ using IE-CSS3 */
    behavior: url(ie-css3.htc);
    padding: 5px 10px;
    display: inline-block;
    position: relative;
    margin-top: 9px;
}

.like_info a {
	color: #F5A313;
}

.arrow_up {
    border-color: transparent transparent #FEFDBA;
    border-style: solid;
    border-width: 0 10px 10px;
    display: block;
    position: absolute;
    height: 0;
    width: 0;
    top: -5px;
    left: 10px;
}

/* error styles */
.errorlist {
    color: red;
    margin: 15px 0px;
}

.errorlist li {
    font-family: "Courier New", "Courier", serif;
    font-weight: bold;
    font-size: 14px;
}

/* photo frame */
.photo_css_frame {
    display: inline-block;
    border: 5px solid #fff;
    -moz-box-shadow: 0 0 3px 2px #999;
    -webkit-box-shadow: 0 0 3px 2px #999;
    box-shadow: 0 0 3px 2px #999;
    behavior: url(ie-css3.htc);
    margin: 5px 10px 10px 5px;
}

